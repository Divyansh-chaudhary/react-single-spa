{
  "name": "react-app",
  "scripts": {
    "dev:standalone": "webpack serve --env standalone",
    "dev:single-spa": "webpack serve",
    "test": "cross-env BABEL_ENV=test jest",
    "build": "webpack --mode=production",
    "lint": "eslint src --ext js,ts,tsx,cts,mts,mjs",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "coverage": "cross-env BABEL_ENV=test jest --coverage",
    "build:types": "tsc",
    "generate": "swagger-typescript-api --union-enums --api-class-name Bff --module-name-index Infinity -p ../common/interface.yml -o src/generated -n index.ts",
    "postinstall": "npm run generate"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@babel/core": "^7.23.3",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/runtime": "^7.23.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^12.0.0",
    "babel-jest": "^27.5.1",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-ts-react-important-stuff": "^3.0.0",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "swagger-typescript-api": "^12.0.4",
    "ts-config-single-spa": "^3.0.0",
    "typescript": "^5.7.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^4.10.0",
    "webpack-config-single-spa-react-ts": "^4.0.2",
    "webpack-dev-server": "^4.0.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@types/jest": "^27.0.1",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/systemjs": "^6.1.1",
    "@types/webpack-env": "^1.16.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^15.2.0",
    "single-spa": "^5.9.3",
    "single-spa-react": "^4.3.1"
  },
  "types": "dist/main.d.ts"
}
